{
  "explorer.width": 40,
  "explorer.icon.expanded": "▾",
  "explorer.icon.collapsed": "▸",
  "explorer.file.column.git.icon.modified": "•",
  "explorer.file.column.git.icon.deleted": "✖",
  "explorer.file.column.git.icon.untracked": "ᵁ",
  "explorer.file.column.git.icon.added": "✚",
  "explorer.file.column.git.icon.renamed": "≫",
  "explorer.file.column.git.icon.unmerged": "≠",
  "explorer.icon.enableNerdfont": true,
  "explorer.previewAction.onHover": false,
  "explorer.quitOnOpen": true,
  "explorer.openAction.for.directory": "expandOrCollapse",
  "explorer.keyMappings": {
    "<cr>": ["expandable?", "expand", "open"],
    "v": "open:vsplit",
    "P": "copyFilepath",
    "F": "copyFilename"
  },
  "list.source.files.defaultOptions": ["--auto-preview"],
  "list.source.outline.defaultOptions": ["--auto-preview"],
  "list.indicator": "❯",
  "list.selectedSignText": " ",
  "suggest.triggerAfterInsertEnter": false,
  "suggest.timeout": 2000,
  "suggest.noselect": false,
  "suggest.triggerCompletionWait": 300,
  "suggest.autoTrigger": "always",
  "suggest.maxCompleteItemCount": 30,
  "suggest.removeDuplicateItems": true,
  "suggest.completionItemKindLabels": {
    "keyword": "",
    "variable": "識",
    "value": "\uf89f",
    "operator": "\u03a8",
    "function": "",
    "reference": "\ufa46",
    "constant": "識",
    "method": "",
    "struct": "\ufb44",
    "class": "\uf0e8",
    "interface": "\uf417",
    "text": "\ue612",
    "enum": "\uf435",
    "enumMember": "\uf02b",
    "module": "\uf40d",
    "color": "\ue22b",
    "property": "",
    "field": "\uf9be",
    "unit": "\uf475",
    "event": "\ufacd",
    "file": "\uf723",
    "folder": "\uf114",
    "snippet": "\ue60b",
    "typeParameter": "\uf728",
    "default": "\uf29c"
  },
  // eslint
  "eslint.run": "onSave",
  "eslint.autoFixOnSave": true,
  "eslint.filetypes": ["javascript", "javascriptreact", "javascript.jsx"],
  //"diagnostic.refreshOnInsertMode": true,
  "diagnostic.signOffset": 9999999,
  "diagnostic.highlightOffset": 9999999,
  "diagnostic.messageDelay": 500,
  "diagnostic.enableSign": true,
  "diagnostic.checkCurrentLine": true,
  "diagnostic.refreshAfterSave": true,
  //"diagnostic.errorSign": " ",
  //"diagnostic.warningSign": " ",
  //"diagnostic.infoSign": "",
  //"diagnostic.hintSign": " ",
  "diagnostic.errorSign": "✘",
  "diagnostic.hintSign": "",
  "diagnostic.infoSign": "",
  "diagnostic.warningSign": "",
  "diagnostic.messageTarget": "echo",
  "diagnostic.virtualText": true,
  "diagnostic.virtualTextPrefix": "❮ ",
  "coc.preferences.jumpCommand": "vsplit",
  "coc.preferences.colorSupport": true,
  "coc.preferences.formatOnType": true,
  "coc.preferences.hoverTarget": "float",
  "coc.preferences.formatOnSaveFiletypes": [
    "javascript",
    "javascriptreact",
    "markdown",
    "typescript",
    "json",
    "html",
    "css",
    "scss",
    "yaml"
  ],
  //"git.addGBlameToBufferVar": true,
  //"git.addGBlameToVirtualText": true,
  //"git.virtualTextPrefix": "        ",
  "git.branchCharacter": "\uf418",
  "git.addedSign.hlGroup": "GitGutterAdd",
  "git.changedSign.hlGroup": "GitGutterChange",
  "git.removedSign.hlGroup": "GitGutterDelete",
  "git.topRemovedSign.hlGroup": "GitGutterDelete",
  "git.changeRemovedSign.hlGroup": "GitGutterChangeDelete",
  "git.conflictedDecorator": "",
  "git.stagedDecorator": "●",
  "git.untrackedDecorator": "",
  "git.addedSign.text": "▋",
  "git.changedSign.text": "▋",
  "git.removedSign.text": "▋",
  "git.topRemovedSign.text": "▔",
  "git.changeRemovedSign.text": "▋",
  "git.enableGutters": false,
  "typescript.showUnused": false,
  //"tsserver.formatOnType": true,
  //"typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBraces": true,
  //"javascript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBraces": true,
  "javascript.showUnused": false,
  "javascript.suggest.autoImports": true,
  "javascript.format.enabled": true,
  "scss.validate": true,
  "html.suggest.html5": true,
  "html.validate.styles": true,
  "html.autoClosingTags": true,
  "html.suggest.ionic": true,
  "prettier.eslintIntegration": true,
  //"prettier.printWidth": 100,
  //"prettier.jsxSingleQuote": true,
  //"prettier.disableSuccessMessage": true,
  //"prettier.stylelintIntegration": true,
  //"prettier.singleQuote": true,
  "snippets.extends": {
    "javascriptreact": ["javascript"]
  },
  "coc-actions.hideCursor": false,
  "yank.highlight.duration": 800,
  "yank.enableCompletion": false
}
