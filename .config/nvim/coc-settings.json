{
  "explorer.width": 40,
  "explorer.icon.enableNerdfont": true,
  "explorer.previewAction.onHover": false,
  "explorer.filename.colored.enable": true,
  "explorer.keyMappings.global": {
    "<cr>": ["expandable?", "expand", "open"],
    "o": ["expandable?", "expand", "open"],
    "v": "open:vsplit",
    "P": "copyFilepath",
    "F": "copyFilename",
    "<space>": "toggleSelection"
  },

  "list.indicator": "❯",
  "list.selectedSignText": " ",
  "list.normalMappings": {
    "<ESC>": "do:exit",
    "<C-c>": "do:exit"
  },

  "suggest.languageSourcePriority": 9999,
  "suggest.triggerAfterInsertEnter": false,
  "suggest.timeout": 2000,
  "suggest.noselect": false,
  "suggest.triggerCompletionWait": 300,
  "suggest.autoTrigger": "always",
  "suggest.maxCompleteItemCount": 20,
  "suggest.removeDuplicateItems": true,
  "suggest.completionItemKindLabels": {
    "keyword": "",
    "variable": "識",
    "value": "\uf89f",
    "operator": "\u03a8",
    "function": "",
    "reference": "\ufa46",
    "constant": "識",
    "method": "",
    "struct": "\ufb44",
    "class": "\uf0e8",
    "interface": "\uf417",
    "text": "\ue612",
    "enum": "\uf435",
    "enumMember": "\uf02b",
    "module": "\uf40d",
    "color": "\ue22b",
    "property": "",
    "field": "\uf9be",
    "unit": "\uf475",
    "event": "\ufacd",
    "file": "\uf723",
    "folder": "\uf114",
    "snippet": "\ue60b",
    "typeParameter": "\uf728",
    "default": "\uf29c"
  },
  // eslint
  "eslint.filetypes": ["javascript", "javascript.jsx", "javascriptreact"],
  "eslint.autoFixOnSave": false,
  "eslint.enable": true,

  "diagnostic.format": "%message [%source]",
  "diagnostic.enableSign": true,
  "diagnostic.errorSign": " ",
  "diagnostic.warningSign": " ",
  "diagnostic.infoSign": "",
  "diagnostic.hintSign": " ",
  "diagnostic.messageTarget": "echo",
  "diagnostic.virtualText": true,
  "diagnostic.enableMessage": "always",
  "diagnostic.checkCurrentLine": true,
  "diagnostic.virtualTextPrefix": "  ❮❮  ",

  "coc.preferences.colorSupport": true,
  "coc.preferences.hoverTarget": "echo",
  "coc.preferences.formatOnType": false,
  "coc.preferences.formatOnSaveFiletypes": ["*"],

  //"git.virtualTextPrefix": "         ",
  "git.virtualTextPrefix": "    ❯❯ ",
  "git.branchCharacter": "\uf418",
  "git.changedDecorator": "!",
  "git.conflictedDecorator": "",
  "git.stagedDecorator": "●",
  "git.untrackedDecorator": "",
  "git.addedSign.text": "│",
  "git.changedSign.text": "│",
  "git.changeRemovedSign.text": "│",
  "git.removedSign.text": "│",
  "git.topRemovedSign.text": "│",
  "git.enableGutters": false,
  "git.semanticCommit.filetypes": ["gitcommit"],

  "jest.terminalPosition": "bottom",
  "tsserver.formatOnType": false,
  //"tsserver.maxTsServerMemory": 512,

  "prettier.eslintIntegration": true,
  //"prettier.stylelintIntegration": true,
  //"stylelintplus.autoFixOnSave": true,
  //"stylelintplus.cssInJs": true,
  //"css.validate": false,
  //"less.validate": false,
  //"scss.validate": false,
  //"wxss.validate": false,
  "snippets.extends": {
    "html": ["html-react"],
    "scss": ["css"],
    "sass": ["css"],
    "less": ["css"],
    "javascript": ["javascript", "react"],
    "typescript": ["javascript", "typescript", "react"],
    "javascriptreact": ["javascript", "react"],
    "vim": ["vim"],
    "json": ["json"]
  },
  "html.suggest.ionic": true,
  "coc-actions.hideCursor": false,
  "yank.highlight.duration": 400,
  "yank.enableCompletion": true,
  "yank.highlight.enable": true,
  "yank.list.maxsize": 50,
  "yank.limit": 50,
  "yaml.schemas": {
    "https://raw.githubusercontent.com/awslabs/goformation/master/schema/cloudformation.schema.json": "/*"
  },
  "yaml.validate": false

  //  "importCost.typescriptExtensions": ["\\.tsx?$"],
  // "importCost.javascriptExtensions": ["\\.jsx?$"],
  // "importCost.bundleSizeDecoration": "both",
  // "importCost.showCalculatingDecoration": true,
  // "importCost.debug": true,
  // "codeLens.enable": true,
  // "javascript.referencesCodeLens.enable": false
}
