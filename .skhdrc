cmd - 1 : yabai -m space --focus 1 && /usr/local/bin/polybar-refresh.sh
cmd - 2 : yabai -m space --focus 2 && /usr/local/bin/polybar-refresh.sh
cmd - 3 : yabai -m space --focus 3 && /usr/local/bin/polybar-refresh.sh
cmd - 4 : yabai -m space --focus 4 && /usr/local/bin/polybar-refresh.sh
cmd - 5 : yabai -m space --focus 5 && /usr/local/bin/polybar-refresh.sh
cmd - 6 : yabai -m space --focus 6 && /usr/local/bin/polybar-refresh.sh
cmd - 7 : yabai -m space --focus 7 && /usr/local/bin/polybar-refresh.sh
cmd - 8 : yabai -m space --focus 8 && /usr/local/bin/polybar-refresh.sh
cmd - 9 : yabai -m space --focus 9 && /usr/local/bin/polybar-refresh.sh
cmd - 0 : yabai -m space --focus 10 && /usr/local/bin/polybar-refresh.sh
cmd - j : yabai -m window --focus next || yabai -m window --focus first
cmd - k : yabai -m window --focus prev || yabai -m window --focus last

cmd - return : alacritty

cmd - space : /Users/void/.config/bin/popup.sh /Users/void/.config/bin/launcher.sh

cmd + shift - space : yabai -m window --toggle float && yabai -m window --toggle topmost && yabai -m window --layer above

